{
	"authors": [
		"Luis Panadero Guardeño"
	],
	"buildTypes": {
		"docs": {
			"buildOptions": [
				"syntaxOnly"
			],
			"dflags": [
				"-c",
				"-Dddocs"
			],
			"dflags-dmd": [
				"-preview=markdown"
			]
		},
		"unittest-opt": {
			"buildOptions": [
				"inline",
				"optimize",
				"unittests"
			]
		}
	},
	"configurations": [
		{
			"name": "ddiv",
			"targetType": "library"
		},
		{
			"dependencies": {
				"pijamas": "~>1.0",
				"silly": "~>1.1"
			},
			"importPaths": [
				"source",
				"tests"
			],
			"name": "unittest",
			"sourcePaths": [
				"source",
				"tests"
			],
			"targetType": "library"
		}
	],
	"copyright": "Copyright © 2020, Luis Panadero Guardeño",
	"dependencies": {
		"bindbc-sdl": "~>0.21",
		"extendedlogger": "~>1.0"
	},
	"description": "DIV inspired 2d game engine",
	"license": "MIT",
	"name": "ddiv",
	"targetPath": "bin",
	"toolchainRequirements": {
		"dub": ">=1.20.0",
		"frontend": ">=2.91.0"
	}
}
