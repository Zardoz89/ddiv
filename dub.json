{
	"authors": [
		"Luis Panadero Guardeño"
	],
	"buildTypes": {
		"docs": {
			"buildOptions": [
				"syntaxOnly"
			],
			"dflags": [
				"-c",
				"-Dddocs"
			],
			"dflags-dmd": [
				"-preview=markdown"
			]
		},
		"unittest-opt": {
			"buildOptions": [
				"inline",
				"optimize",
				"unittests"
			]
		}
	},
	"configurations": [
		{
            "name": "ddiv",
            "targetType": "library"
        },
		{
			"dependencies": {
				"silly": "~>1.0.2",
				"beep": "~>0.0.2",
				"pijamas": "~develop"
			},
			"name": "unittest",
			"targetType": "library",
			"importPaths": ["source", "tests"],
            "sourcePaths": ["source", "tests"]
		}
	],
	"copyright": "Copyright © 2020, Luis Panadero Guardeño",
	"dependencies": {
		"bindbc-sdl": "~>0.16.0",
		"stdx-allocator": "~>3.0.2"
	},
	"description": "DIV inspired 2d game engine",
	"license": "MIT",
	"name": "ddiv",
	"targetPath": "bin",
    "toolchainRequirements": {
        "dub": ">=1.20.0",
        "frontend": ">=2.091"
    }
}
